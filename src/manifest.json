{
  "manifest_version": 2,
  "name": "soundcloud-remember-position",
  "description": "Saves your soundcloud playback position to local storage",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "https://soundcloud.com/*"
  ],
  "web_accessible_resources": [
    "inject.js"
  ],
  "content_scripts": [
    {
      "matches": ["https://soundcloud.com/*"],
      "js": ["content.js"]
    }
  ]
}